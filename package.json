{
	"name": "happyprime",
	"version": "1.0.0",
	"description": "",
	"devDependencies": {
	  "@wordpress/scripts": "^27.0.0",
	  "@wordpress/stylelint-config": "^21.32.0",
	  "autoprefixer": "^10.4.16",
	  "postcss": "^8.4.33",
	  "postcss-advanced-variables": "^3.0.1",
	  "postcss-cli": "^11.0.0",
	  "postcss-custom-media": "^10.0.2",
	  "postcss-import": "^16.0.0",
	  "postcss-nested": "^6.0.1",
	  "postcss-preset-env": "^9.3.0"
	},
	"scripts": {
	  "build": "npm run build:css && npm run build:blocks",
	  "build:css": "npm run stylelint && npm run postcss:main",
	  "build:js": "npm run build:theme:js",
	  "build:blocks": "wp-scripts build --config mu-plugins/happyprime/blocks/webpack.config.js",
	  "build:theme:js": "wp-scripts build --config themes/happyprime/webpack.config.js",
	  "postcss:main": "postcss -o themes/happyprime/style.css themes/happyprime/css/style.css",
	  "postcss:editor": "postcss -o themes/happyprime/editor-style.css themes/happyprime/css/editor-style.css",
	  "stylelint": "stylelint themes/happyprime/css/**/*.css",
	  "lint:js": "wp-scripts lint-js",
	  "lint:blocks:js": "wp-scripts lint-js mu-plugins/happyprime/blocks/",
	  "lint:theme:js": "wp-scripts lint-js --config themes/happyprime/webpack.config.js",
	  "fix:js": "wp-scripts lint-js --fix",
	  "fix:blocks:js": "wp-scripts lint-js mu-plugins/happyprime/blocks/ --fix"
	},
	"repository": {
	  "type": "git",
	  "url": "git+https://github.com/happyprime/happyprime.co.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
	  "url": "https://github.com/happyprime/happyprime.co/issues"
	},
	"homepage": "https://github.com/happyprime/happyprime.co#readme"
  }
